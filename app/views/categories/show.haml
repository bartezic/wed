%h1= @category.name
.row
  - @category.partners.each do |partner|
    .col-xs-4.col-sm-4.col-md-3.col-lg-2
      = link_to partner do
        = image_tag(partner.avatar(:thumb_220), { class: :'img-circle img-responsive', alt: partner.name })
        %div= partner.name
        %div= raw "від <b>#{partner.price}</b>$"